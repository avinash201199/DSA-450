
#include<stdio.h>

int main()
{
    int a[100],i,n,j,temp=0;
    printf("enter the no of element");
    scanf("%d",&n);
    printf("enter the numbers");
    for(i=0;i<n;i++)

        scanf("%d",&a[i]);
        for(j=0;j<n;j++)
        {
            for(i=j+1;i<n;i++)
            {
                if(a[j]==a[i])
                   {
                        printf("duplicate number find at position %d and %d",i+1,j+1);
                        temp=1;
                   }

            }
        }
        if(temp==0)
            printf("No duplicate number");



}
